<project name="WebServer" default="run">

<target name="compile">
    <javac srcdir=".\SimpleWebserver" />
</target>

<target name="run" depends="compile">
    <java classname="WebServer"/>
</target>

<target name="clean">
    <delete>
        <fileset dir="." includes="*.class"/>
    </delete>
</target>

</project>